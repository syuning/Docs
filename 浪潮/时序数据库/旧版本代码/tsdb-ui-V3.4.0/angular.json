{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "tsdb": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/config",
              "src/assets",
              "src/favicon.ico",
              "src/apis",
              "src/cks"
            ],
            "styles": [
              "node_modules/ng-zorro-antd/src/ng-zorro-antd.less",
              "src/assets/css/bootstrap/dist/css/bootstrap.min.css",
              "src/assets/css/iconfont/iconfont.css",
              "src/assets/font-awesome/web-fonts-with-css/css/fontawesome-all.min.css",
              "node_modules/ng-zorro-iop/css/style.less",
              "node_modules/ng-zorro-iop/css/boss-common.css",
              "src/styles.css"
            ],
            "scripts": [
              "node_modules/echarts/dist/echarts.min.js"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "tsdb:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "tsdb:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "tsdb:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "node_modules/echarts/dist/echarts.min.js"
            ],
            "styles": [
              "node_modules/ng-zorro-antd/src/ng-zorro-antd.less",
              "src/assets/css/bootstrap/dist/css/bootstrap.min.css",
              "src/assets/css/iconfont/iconfont.css",
              "src/assets/font-awesome/web-fonts-with-css/css/fontawesome-all.min.css",
              "node_modules/ng-zorro-iop/css/style.less",
              "node_modules/ng-zorro-iop/css/boss-common.css",
              "src/styles.css"
            ],
            "assets": [
              "src/config",
              "src/assets",
              "src/favicon.ico",
              "src/apis",
              "src/cks"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "tsdb-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "tsdb:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "tsdb",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "styleext": "css"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}
