_HOME/server/scripts/cloud-scripts/zkcli.sh -zkhost  $ZK_HOST:2181 -cmd put=
 /solr/security.json '{"authentication":{"class": "org.apache.solr.security=
.KerberosPlugin"},"authorization":{"class": "org.apache.ranger.authorizatio=
n.solr.authorizer.RangerSolrAuthorizer"}}'

